{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/errors",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/no-default-export": "error"
  },
  "settings": {
    "import/resolver": "typescript"
  }
}
