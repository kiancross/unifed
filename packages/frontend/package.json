    "react": "^16.14.0",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.14.0",
    "react-markdown": "^5.0.2",
    "react-markdown-editor-lite": "^1.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
