{
  "name": "unifed-frontend",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "engines": {
    "node": ">=10.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "yarn workspace unifed-shared build && react-scripts build",
    "test:unit": "react-scripts test --coverage --coverageReporters html json text --watchAll=false",
    "test:integration": "cypress run",
    "test:integration:open": "cypress open",
    "clean": "rm -rf coverage .nyc_output build cypress/videos cypress/screenshots",
    "lint": "eslint --ext .js,.ts,.tsx src/ cypress/",
    "fix": "eslint --ext .js,ts,tsx --fix src/ cypress/"
  },
  "dependencies": {
    "@accounts/apollo-link": "^0.29.0",
