{
  "name": "@unifed/backend-ml",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mailparser": "^3.0.1",
    "@types/node": "^14.14.28",
    "@types/source-map-support": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "ava": "^3.15.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow/tfjs-converter": "^2.8.6",
    "@tensorflow/tfjs-core": "^2.8.6",
    "@tensorflow/tfjs-node-gpu": "^3.1.0",
    "csv-parse": "^4.15.1",
    "mailparser": "^3.1.0",
    "node-stream-zip": "^1.13.1"
  },
  "scripts": {
    "train": "node --max-old-space-size=25600 build/train.js",
    "test-model": "node build/test-model.js",
    "test": "nyc ava 'src/**/*.test.ts'",
    "build": "tsc",
    "clean": "rm -rf coverage build .nyc_output tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix"
  },
  "ava": {
    "timeout": "2m",
    "files": [
      "!build/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
